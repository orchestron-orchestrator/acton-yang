def take_node(stmts: list[Statement], kw: str, parent: ?SchemaNode=None) -> ?SchemaNode:
    for i in range(len(stmts)):
        if stmts[i].kw == kw:
            n = stmt_to_snode(stmts[i], parent=parent)
