        self.reference = reference
        self.status = status
        self.type_ = type_
        self.units = units
        self.when = when
        self.exts = exts

    def is_key(self) -> bool:
        parent = self.parent
        if isinstance(parent, DList):
            # If the leaf is in a different namespace as the list, it:
            # - cannot possibly be a key,
            # - may even have the same name as one of the keys!
            if self.namespace == parent.namespace:
                if self.name in parent.key:
                    return True
        return False

    def prdaclass(self, schema_yang: ?list[str]=None, loose=False, top=True, set_ns=True, schema_ns=set(), gen_json=True, gen_xml=True, include_state=False) -> str:
        if self.namespace != "":
            schema_ns.add(self.namespace)
        if include_state == False and self.config == False:
            return ""
        def pname(n):
            return get_path_name(n)
        # Include namespace qualifier(s) iff our namespace is different than our
        # parent
        gdata_nsq = ", ns='{self.namespace}', module='{self.module}'" if set_ns else ""
        res = []
        if gen_json:
            if self.type_.name == "identityref":
                res.append("mut def from_json_{pname(self)}(val: PartialIdentityref) -> yang.gdata.Leaf:")
                res.append("    new_val, error = complete_and_validate_identityref(val, _identities, {repr(self.type_.base)}, {repr(self.module)})")
                res.append("    if new_val is not None:")
                res.append("        return yang.gdata.Leaf('{self.type_.name}', new_val{gdata_nsq})")
                res.append("    else:")
                res.append("        raise ValueError('Invalid value for identityref leaf {self.name}: {{error}}')")
            else:
                res.append("mut def from_json_{pname(self)}(val: value) -> yang.gdata.Leaf:")
                res.append("    return yang.gdata.Leaf('{self.type_.name}', val{gdata_nsq})")
            res.append("")
        if gen_xml:
            if self.type_.name == "identityref":
                res.append("mut def from_xml_{pname(self)}(val: PartialIdentityref) -> yang.gdata.Leaf:")
                res.append("    new_val, error = complete_and_validate_identityref(val, _identities, {repr(self.type_.base)}, {repr(self.module)})")
                res.append("    if new_val is not None:")
                res.append("        return yang.gdata.Leaf('{self.type_.name}', new_val{gdata_nsq})")
                res.append("    else:")
                res.append("        raise ValueError('Invalid value for identityref leaf {self.name}: {{error}}')")
            else:
                res.append("mut def from_xml_{pname(self)}(val: value) -> yang.gdata.Leaf:")
                res.append("    return yang.gdata.Leaf('{self.type_.name}', val{gdata_nsq})")
            res.append("")
        return "\n".join(res)


