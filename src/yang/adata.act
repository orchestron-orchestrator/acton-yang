
import yang.schema
import yang.data

"""Base classes for YANG-modeled data classes (MData)
"""

class MNode(object):
    _schema: ?yang.schema.SchemaNode
    _name: str
    _parent: ?MNode

    def __init__(self, _name: str, _parent: ?MNode=None):
        self._name = _name
        self._parent = _parent
        self._schema = None

    mut def to_gdata(self) -> yang.data.Node:
        raise NotImplementedError("MNode.to_gdata")
