class DLeaf(DNodeLeaf):
    default: ?str
    mandatory: bool
    units: ?str

    def __init__(self, module: str, namespace: str, prefix: str, name: str, config: bool, description: ?str=None, default=None, if_feature=[], mandatory=False, must=[], reference=None, status=None, type_: Type, units=None, when=None, exts=[]):
        self.parent = None
        self.module = module
        self.namespace = namespace
        self.prefix = prefix
        self.name = name
        self.gname = "Leaf"
        self.config = config
        self.description = description
        self.default = default
        self.if_feature = if_feature
        self.mandatory = mandatory
        self.must = must
        self.reference = reference
        self.status = status
        self.type_ = type_
        self.units = units
        self.when = when
        self.exts = exts

    def is_key(self) -> bool:
        parent = self.parent
        if isinstance(parent, DList):
            # If the leaf is in a different namespace as the list, it:
            # - cannot possibly be a key,
            # - may even have the same name as one of the keys!
            if self.namespace == parent.namespace:
                if self.name in parent.key:
                    return True
        return False

    def _prdaclass_rec(self, spath: list[DNode], schema_yang: ?list[str]=None, loose=False, top=True, set_ns=True, schema_ns=set(), gen_json: bool=True, gen_xml: bool=True, include_state=False) -> str:
        if self.namespace != "":
            schema_ns.add(self.namespace)
        if include_state == False and self.config == False:
            return ""
        def pname(n):
            return get_path_name(n)
        # Include namespace qualifier(s) iff our namespace is different than our parent
        gdata_nsq = ", ns='{self.namespace}', module='{self.module}'" if set_ns else ""
        res = []
        if gen_json or gen_xml:
            res.append("mut def from_data_{pname(self)}(val: value) -> yang.gdata.Leaf:")
            if self.type_.name == "identityref":
                res.append("    identityref, error = complete_and_validate_identityref(val, _identities, {repr(self.type_.base)}, {repr(self.module)})")
                res.append("    if identityref is not None:")
                res.append("        return yang.gdata.Leaf(identityref{gdata_nsq})")
                res.append("    raise ValueError('Invalid value for identityref leaf {self.name}: {{error}}')")
            else:
                # TODO: what if the union includes identityref? Then try_parse_value should resolve identities ...
                res.append("    tv = yang.gen3.try_parse_value(yang.gdata.yang_str(val), {self.type_.prsrc()}, [])")
                res.append("    return yang.gdata.Leaf(tv.val{gdata_nsq})")
            res.append("")
            res.append("")
        return "\n".join(res)


class DLeafList(DNodeLeaf):
    default: ?list[str]
    max_elements: ?int
    min_elements: int
    ordered_by: str
    units: ?str

    def __init__(self, module: str, namespace: str, prefix: str, name: str, config: bool, description: ?str=None, default=None, if_feature=[], max_elements: ?int=None, min_elements=0, must=[], ordered_by="system", reference=None, status=None, type_: Type, units=None, when=None, exts=[]):
        self.parent = None
        self.module = module
        self.namespace = namespace
        self.prefix = prefix
        self.name = name
        self.gname = "LeafList"
        self.config = config
        self.description = description
        self.default = default
        self.if_feature = if_feature
        self.max_elements = max_elements
        self.min_elements = min_elements
        self.must = must
        self.ordered_by = ordered_by
        self.reference = reference
        self.status = status
        self.type_ = type_
        self.units = units
        self.when = when
        self.exts = exts

    def _prdaclass_rec(self, spath: list[DNode], schema_yang: ?list[str]=None, loose=False, top=True, set_ns=True, schema_ns=set(), gen_json: bool=True, gen_xml: bool=True, include_state=False) -> str:
        if self.namespace != "":
            schema_ns.add(self.namespace)
        if include_state == False and self.config == False:
            return ""
        def pname(n):
            return get_path_name(n)
        maybe_user_order = ", user_order=True" if self.ordered_by == "user" else ""
        # Include namespace qualifier(s) iff our namespace is different than our parent
        gdata_nsq = ", ns='{self.namespace}', module='{self.module}'" if set_ns else ""
        res = []
        if gen_json or gen_xml:
            # val: list[value] contains values of the same type, guaranteed in from_XXX_YYYs() -> list[YYY]
            res.append("mut def from_data_{pname(self)}(val: list[value]) -> yang.gdata.LeafList:")
            if self.type_.name in {"union", "identityref"}:
                res.append("    new_val = []")
                if self.type_.name == "union":
                    res.append("    for v in val:")
                    if self.type_.name == "union":
                        res.append("        tv = yang.gen3.try_parse_value(yang.gdata.yang_str(val), {self.type_.prsrc()}, [])")
                        res.append("        new_val.append(tv.val)")
                else:
                    res.append("    new_type = '{self.type_.name}'")
                    res.append("    for v in val:")
                    res.append("        nv, error = complete_and_validate_identityref(v, _identities, {repr(self.type_.base)}, {repr(self.module)})")
                    res.append("        if nv is not None:")
                    res.append("            new_val.append(nv)")
                    res.append("        else:")
                    res.append("            raise ValueError('Invalid value for identityref leaf-list {self.name}: {{error}}')")
                res.append("    return yang.gdata.LeafList(new_val{maybe_user_order}{gdata_nsq})")
            else:
                res.append("    return yang.gdata.LeafList(val{maybe_user_order}{gdata_nsq})")
            res.append("")
        return "\n".join(res)


