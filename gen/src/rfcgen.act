    "type": {
        "resolve_union_types": r"""    def resolve_union_types(self, context: Context) -> Type:
        if self.name == "union":
            # Resolve union types
            resolved_union_types = [ut.resolve_typedef(context).type_ for ut in self.type_]
            new = Type(self.name,
                       base=self.base,
                       bit=self.bit,
                       enum=self.enum,
                       fraction_digits=self.fraction_digits,
                       length=self.length,
