Container({
  'c1': Container({
    'li': List(['name'], user_order=True),
    'll_uint64': LeafList('uint64', []),
    'll_str': LeafList('string', []),
    'bar:l1': Leaf('string', 'foo-bar', ns='http://example.com/bar', module='bar'),
    'l2': Leaf('string', 'bar', ns='http://example.com/bar', module='bar')
  }, ns='http://example.com/foo'),
  'c.dot': Container(ns='http://example.com/foo'),
  'cc': Container({
    'death': List(['name'])
  }, ns='http://example.com/foo'),
  'f:conflict': Container(ns='http://example.com/foo'),
  'special': List(['yes'], ns='http://example.com/foo', module='foo'),
  'nested': Container({
    'f:inner': Container({
      'li1': List(['name'])
    }),
    'bar:inner': Container(ns='http://example.com/bar')
  }, ns='http://example.com/foo'),
  'li-union': List(['k1', 'k2', 'k3'], ns='http://example.com/foo', module='foo'),
  'state': Container({
    'c1': Container()
  }, ns='http://example.com/foo'),
  'c2': Container(ns='http://example.com/foo'),
  'bar:conflict': Container(ns='http://example.com/bar')
})