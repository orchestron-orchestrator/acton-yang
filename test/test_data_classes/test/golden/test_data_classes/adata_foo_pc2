Container({
  'c1': Container(ns='http://example.com/foo', module='foo'),
  'pc2': Container({
    'foo': Container({
      'l_mandatory': Leaf('baz')
    })
  }, presence=True, ns='http://example.com/foo', module='foo'),
  'c.dot': Container(ns='http://example.com/foo', module='foo'),
  'cc': Container(ns='http://example.com/foo', module='foo'),
  'f:conflict': Container(ns='http://example.com/foo', module='foo'),
  'nested': Container({
    'f:inner': Container(),
    'bar:inner': Container(ns='http://example.com/bar', module='bar')
  }, ns='http://example.com/foo', module='foo'),
  'state': Container({
    'c1': Container()
  }, ns='http://example.com/foo', module='foo'),
  'c2': Container(ns='http://example.com/foo', module='foo'),
  'test-idref': Container(ns='http://example.com/bar', module='bar'),
  'bar:conflict': Container(ns='http://example.com/bar', module='bar')
})