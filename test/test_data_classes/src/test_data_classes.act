def _test_json_path():
    jda = {
        "foo:nested": {
            "inner": {
                "foo": "WINNING"
            }
        }
    }
    gda = yang_foo.from_json(jda)

    jdb = {
        "foo": "WINNING"
        }
    gdb = yang_foo.from_json_path(jdb, ["foo:nested", "inner"])

    testing.assertEqual(gda.prsrc(), gdb.prsrc())

def _test_json_path_nested_key_in_payload():
