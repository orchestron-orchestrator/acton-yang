# YANG model used to generate yang_foo
#
# module foo {
#     yang-version "1.1";
#     namespace "http://example.com/foo";
#     prefix "foo";
#     container c1 {
#         leaf l1 {
#             type string;
#         }
#         leaf l3 {
#             type uint64;
#         }
#         leaf l_empty {
#             type empty;
#         }
#         list li {
#             key name;
#             leaf name {
#                 type string;
#             }
#             leaf val {
#                 type string;
#             }
#         }
#         leaf-list ll_uint64 {
#             type uint64;
#         }
#         leaf-list ll_str {
#             type string;
#         }
#     }
#     container pc1 {
#         presence "p";
#         container foo {
#             leaf l1 {
#                 type binary;
#             }
#         }
#     }
#     container c.dot {
#         leaf l.dot1 {
#             type string;
#         }
#     }
#     list special {
#         key yes;
#         leaf yes {
#             type boolean;
#         }
#     }
# }

