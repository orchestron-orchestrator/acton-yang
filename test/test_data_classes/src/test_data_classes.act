def _test_json_path_nested_key_in_payload():
    jda = {
        "foo:nested": {
            "inner": {
                "li1": [
                    {
                        "name": "AAA",
                        "bar": "WINNING"
                    }
                ]
            }
        }
    }
    gda = yang_foo.from_json(jda)

    jdb = {
        "name": "AAA",
        "bar": "WINNING"
        }
    gdb = yang_foo.from_json_path(jdb, ["foo:nested", "inner", "li1", "AAA"])

    testing.assertEqual(gda.prsrc(), gdb.prsrc())
    return gdb.prsrc()

