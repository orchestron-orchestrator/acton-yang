module test_choice {
  yang-version "1.1";
  namespace "http://example.com/test_choice";
  prefix "test";
  container config {
    choice protocol {
      case http {
        leaf http {
          type empty;
        }
      }
      case https {
        container https {
          leaf port {
            type uint16;
          }
        }
      }
      case ssh {
        leaf ssh-port {
          type uint16;
        }
      }
    }
  }
}
