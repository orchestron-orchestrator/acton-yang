Container({
  'config': Container({
    'delete_node': Delete(module='test'),
    'create_node': Create({
      'new_leaf': Leaf('str', 'created')
    }, module='test'),
    'replace_node': Replace({
      'replaced_leaf': Leaf('int', 100)
    }, module='test'),
    'absent_node': Absent({
      'removed_leaf': Leaf('str', 'gone')
    }, module='test')
  })
})