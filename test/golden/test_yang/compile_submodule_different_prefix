mut def from_json_path_main_module__main_container(jd: value, path: list[str]=[], op: ?str='merge') -> yang.gdata.Node:
    # path handling
    if len(path) > 0:
        point = path[0]
        rest_path = path[1:]
        if point == 'main-leaf':
            raise ValueError("Invalid json path to non-inner node")
